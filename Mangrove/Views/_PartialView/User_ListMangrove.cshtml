@inject IStringLocalizer<Translate> translate
@model List<SearchMangroe_Client_VM>
@{
    if (Model.Count > 0)
    {
        long stt = 0;
        foreach (var item in Model)
        {
            stt += 1;

            <a href='@Url.Action("Page_Result", "Home", new { id = @item.Id })'
               class="item_mangrove d-block text-start py-1 px-2 mb-2 rounded-1 overflow-hidden time_delay_500ms btn_loading">
                <h5 class="mb-2 name">#@stt | @item.Name</h5>
                <p class="content">@item.Morphology</p>
            </a>
        }
    }
    else
    {
        <p class="text-center">@translate["label_khong_tim_thay_ket_qua"]</p>
    }
}